{
	"jsRules": {},
	"rules": {
		"indent": [true, "tabs"],
		"quotemark": {
				"options": [true, "single"],
				"severity": "warning"
		},
		"prefer-const": {
			"options": true,
			"severity": "warning"
		},
		"no-unused-variable": {
				"options": [true, {"ignore-pattern": "^_"}],
				"severity": "warning"
		},
		"triple-equals": [true, "allow-null-check", "allow-undefined-check"],
		"no-var-keyword": true,
		"curly": [true, "ignore-same-line"],
		"one-line": {
			"options": [true, "check-if", "check-catch", "check-finally", "check-else"],
			"severity": "warning"
		},
		"no-empty": {
			"options": true,
			"severity": "warning"
		},
		"no-eval": true,
		"one-variable-per-declaration": {
			"options": true,
			"severity": "warning"
		},
		"no-conditional-assignment": true,
		"import-spacing": true,
		"arrow-parens": true,
		"new-parens": true,
		"no-duplicate-super": true,
		"no-sparse-arrays": true,
		"no-duplicate-switch-case": true,
		"no-duplicate-variable": true,
		"no-for-in": true,
		"unnecessary-bind": true,
		"no-duplicate-imports": [true, {"allow-namespace-imports": true}],
		"space-before-function-paren": {"options": true, "severity":"warning"},
		"no-shadowed-variable": {"options": true, "severity":"warning"},
		"prefer-readonly": {"options": true, "severity":"warning"},
		"unnecessary-else": [true, {"allow-else-if": true}],
		"no-unsafe-finally": true,
		"number-literal-format": true,
		"no-parameter-reassignment": true,
		"label-position": true,
		"no-misused-new": true,
		"whitespace": {
				"options": [true, "check-branch","check-decl","check-operator","check-separator","check-rest-spread","check-type","check-typecast","check-type-operator"],
				"severity": "warning"
		},
		"array-type": [true, "array"]
	},
	"rulesDirectory": []
}
